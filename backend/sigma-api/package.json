{
	"name": "sigma_project",
	"scripts": {
		"build": "rm -rf build && tsc",
		"start": "tsnd src/index.ts",
		"typeorm": "tsnd ./node_modules/typeorm/cli.js",
		"migration:create": "npm run typeorm migration:create ./src/db/migrations/$npm_config_name",
		"migration:run": "yarn typeorm migration:run -d ./src/db/coreConnection.ts",
		"migration:revert": "yarn typeorm migration:revert -d ./src/db/coreConnection.ts"
	},
	"version": "1.0.0",
	"description": "Sigma Project for IoT ",
	"main": "index.js",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@types/node": "^22.10.7",
		"@types/swagger-ui-express": "4.1.7",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-prettier": "5.2.1",
		"globals": "^15.14.0",
		"prettier": "3.4.2",
		"swagger-jsdoc": "6.2.8",
		"swagger-ui-express": "5.0.1",
		"ts-node-dev": "2.0.0",
		"typescript-eslint": "^8.20.0"
	},
	"dependencies": {
		"dotenv": "^16.4.7",
		"express": "^4.18.2",
		"ioredis": "^5.4.2",
		"mqtt": "^5.10.3",
		"pg": "^8.13.1",
		"reflect-metadata": "^0.2.2",
		"ts-node": "^10.9.2",
		"typeorm": "^0.3.20",
		"typescript": "^5.7.3",
		"zod": "^3.24.1"
	},
	"private": true
}
